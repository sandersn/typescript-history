module Test 
{
    export class User 
    {
        ID: number;
        Name: string;

        constructor (id: number, name: string) 
        {
            this.ID = id;
            this.Name = name;
        }
        
        ToString() 
        {
            return "ID: " + this.ID.toString() + " Name: " + this.Name;
        }
    }
}